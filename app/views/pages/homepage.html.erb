<div id="buttonmix-main">
  <% @links.each do |adviser| %>
      <div class="col-xs-4" >
          <a class="col-xs-12 btn btn-yellow" style="height: 230px; margin-top: 30px" href="<%= adviser.link%>">
            <h1><strong><%= adviser.name %></strong></h1>
            <h5><%= adviser.description %></h5>
          </a>
      </div>
  <% end %>
</div>